wait()getgenv().i=true;local a=game.Players.LocalPlayer;local b="Slot1"local c="SLOT1"local d="SLOT 1: "local e="Mirage Of Phantoms(The World Over Heaven)"local f="Epic"local g=true;game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(h)if h=="u"then local i=a.PlayerGui.HUD.Main.Frames.Stands;local j;local k;local l;for m,n in pairs(i.ScrollingFrame:GetChildren())do if n.Name==b then if n:FindFirstChild("Default")then for m,o in pairs(i:GetDescendants())do if o.Name=="Shiny"then if n:FindFirstChild("Default")then n.Default:Destroy()end;if n.TextLabel:FindFirstChild("Default")then n.TextLabel.Default:Destroy()end;o:Clone().Parent=n;o:Clone().Parent=n.TextLabel end end end;n.TextLabel.Text=d..e end end end end)local p;if f=="Uncommon"then p='RARITY: <font color="rgb(177, 229, 166)">Uncommon</font>'elseif f=="Epic"then p='RARITY: <font color="rgb(75, 0, 130)">Epic</font>'end;a.PlayerGui.ChildAdded:Connect(function(q)if getgenv().i==true then if q.Name=="CosmeticTrade"then wait()q.Main.Stands.ScrollingFrame.UIGridLayout.SortOrder=0;print(1)local r=q.Main.Stands:WaitForChild("Sample"):Clone()r.Name=b;r.Square:WaitForChild("Title").Text=e;r.Parent=q.Main.Stands.ScrollingFrame;r.Square.Rarity.Text=p;r.Visible=true;r.Square.Quantity.Text=c;if g==true then r.Square.Unique.Visible=true end;if q.Main.Stands.ScrollingFrame:FindFirstChild("Stand")then local s=q.Main.Stands.ScrollingFrame:FindFirstChild("Stand"):Clone()s.Name="Slot0"s.Parent=q.Main.Stands.ScrollingFrame;print(1)wait()q.Main.Stands.ScrollingFrame:FindFirstChild("Stand").Visible=false end;q.Main.Stands.ScrollingFrame.UIGridLayout.SortOrder=0 end end end)
